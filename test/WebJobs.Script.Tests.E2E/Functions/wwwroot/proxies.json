{
  "proxies": {
    "FileExtensionCheck": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "test.txt"
      },
      "responseOverrides": {
        "response.statusCode": "200",
        "response.headers.Content-Type": "text/plain",
        "response.body": "test"
      }
    },
    "RootCheck": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/"
      },
      "responseOverrides": {
        "response.headers.Content-Type": "text/plain",
        "response.body": "Root"
      }

    },
    "LocalFunctionCall": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/myhttptrigger"
      },
      "backendUri": "https://localhost/api/Ping",
      "requestOverrides": {
        "backend.request.headers.accept": "text/plain"
      }
    }
  }
}