{
  "config": {
    "queues": {
      "maxPollingInterval": 2000
    },
    "tracing": {
      "consoleLevel": "Verbose"
    },
    "webhooks": {
      "port": 45000
    }
  },
  "functions": [
    {
      "source": "ScheduledJob.cs",
      "trigger": {
        "type": "timer",
        "schedule": "* * * * * *"
      }
    },
    {
      "source": "WebHook.cs",
      "trigger": {
        "type": "webhook"
      }
    }
  ]
}