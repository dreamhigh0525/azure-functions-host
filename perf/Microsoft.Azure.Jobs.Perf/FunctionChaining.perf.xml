<?xml version="1.0" encoding="utf-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.microsoft.com/2011/12/aaptperffun#1">
  <metadata
      testName ="FunctionChaining"
      teamName ="SimpleBatch"
      description="Queue function chaining">

    <trackingVariation name="HostStart">
      <metric name="TotalElapsedTime" selection="Median" units="Milliseconds">
        <target value="1500" labConfig="default" priority = "1" />
      </metric>
    </trackingVariation>
    <trackingVariation name="QueueChain">
      <metric name="TotalElapsedTime" selection="Median" units="Milliseconds">
        <target value="7500" labConfig="default" priority = "1" />
      </metric>
    </trackingVariation>
  </metadata>

  <commands>
    <command name="RunTest" wait="Exit">
      Microsoft.Azure.Jobs.Perf.exe /StorageConnectionString={ConnectionString} /Scenario=FunctionChaining
    </command>
  </commands>

  <plan>
    <run command="RunTest" role="client" output="true" measure="StartAndStop"  />
  </plan>
</config>