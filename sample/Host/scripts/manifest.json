{
  "config": {
    "queues": {
      "maxPollingInterval": 2000
    },
    "tracing": {
      "consoleLevel": "Verbose"
    }
  },
  "functions": [
    {
      "source": "QueueTrigger.cs",
      "trigger": {
        "type": "queue",
        "queueName": "samples-posts"
      }
    },
    {
      "source": "BlobTrigger.cs",
      "trigger": {
        "type": "blob",
        "blobPath": "samples-workitems"
      }
    },
    {
      "source": "TimerTrigger.cs",
      "trigger": {
        "type": "timer",
        "schedule": "*/10 * * * * *"
      }
    },
    {
      "source": "WebHookTrigger.cs",
      "trigger": {
        "type": "webHook"
      }
    }
  ]
}