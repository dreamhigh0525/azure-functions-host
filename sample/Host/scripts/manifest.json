{
  "config": {
    "queues": {
      "maxPollingInterval": 2000
    },
    "tracing": {
      "consoleLevel": "Verbose"
    }
  },
  "functions": [
    {
      "source": "ProcessPost.cs",
      "trigger": {
        "type": "queue",
        "queueName": "samples-posts"
      }
    },
    {
      "source": "ScheduledJob.cs",
      "trigger": {
        "type": "timer",
        "schedule": "*/10 * * * * *"
      }
    },
    {
      "source": "WebHook.cs",
      "trigger": {
        "type": "webHook"
      }
    }
  ]
}