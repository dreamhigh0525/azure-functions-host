{
    "bindings": [
        {
            "type": "eventHubTrigger",
            "name": "input",
            "direction": "in",
            "connection": "AzureWebJobsEventHubReceiver",
            "path": "%AzureWebJobsEventHubPath%"
        },
        {
            "type": "eventHub",
            "name": "output",
            "direction": "out",
            "connection": "AzureWebJobsEventHubSender",
            "path": "%AzureWebJobsEventHubPath%"
        }
    ]
}
