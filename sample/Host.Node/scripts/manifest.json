{
  "config": {
    "queues": {
      "maxPollingInterval": 2000
    },
    "tracing": {
      "consoleLevel": "Verbose"
    }
  },
  "functions": [
    {
      "source": "scheduledJob.js",
      "trigger": {
        "type": "timer",
        "schedule": "*/10 * * * * *"
      }
    },
    {
      "source": "processWorkItem.js",
      "trigger": {
        "type": "queue",
        "queueName": "samples-workitems"
      }
    },
    {
      "source": "webHook.js",
      "trigger": {
        "type": "webhook"
      }
    }
  ]
}