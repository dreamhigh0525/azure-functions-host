{
  "config": {
    "queues": {
      "maxPollingInterval": 2000
    },
    "tracing": {
      "consoleLevel": "Verbose"
    },
    "singleton": {
      "listenerLockPeriod":  "00:00:15"
    }
  },
  "functions": [
    {
      "source": "timerTrigger.js",
      "trigger": {
        "type": "timer",
        "schedule": "*/15 * * * * *",
        "runOnStartup": true
      }
    },
    {
      "source": "queueTrigger.js",
      "trigger": {
        "type": "queue",
        "queueName": "samples-workitems"
      }
    },
    {
      "source": "blobTrigger.js",
      "trigger": {
        "type": "blob",
        "blobPath": "samples-workitems"
      }
    },
    {
      "source": "webHookTrigger.js",
      "trigger": {
        "type": "webHook"
      }
    }
  ]
}